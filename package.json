{
	"name": "vscode-issue-79705",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.33.0"
	},
	"categories": ["Other"],
	"contributes": {
		"languages": [
			{
				"id": "haxe",
				"extensions": [
					".hx"
				]
			}
		],
		"grammars": [
			{
				"language": "haxe",
				"scopeName": "source.hx",
				"path": "haxe.tmLanguage.json"
			},
			{
				"scopeName": "markdown.haxe.codeblock",
				"path": "haxe-markdown.tmLanguage.json",
				"injectTo": [
					"text.html.markdown",
					"source.hx"
				],
				"embeddedLanguages": {
					"meta.embedded.block.haxe": "haxe"
				}
			},
			{
				"scopeName": "documentation.markdown.injection.haxe",
				"path": "haxe-markdown-docs.tmLanguage.json",
				"injectTo": [
					"source.hx"
				]
			}
		]
	}
}
